<!DOCTYPE skin>
<skin>

 <Schemes>
  <Scheme>
   <Name>11pm (Dark Mixxx)</Name>
   <Filters>
    <Invert/>
	<HueInv/>
    <Add>
     <Amount>50</Amount>
    </Add>
    <ScaleWhite>
     <Amount>1.5</Amount>
    </ScaleWhite>
    <Add>
     <Amount>-50</Amount>
    </Add>
   </Filters>
  </Scheme> 
  <Scheme>
   <Name>5pm (Classic Mixxx)</Name>
   <Filters>
   </Filters>
  </Scheme>
	 <Scheme>
		 <Name>8pm (Summer Sunset Mixxx)</Name>
		 <Filters>
			 <HSVTweak>
				 <SMin>100</SMin>
				 <VFact>0.7</VFact>
				 <HFact>0.3</HFact>
			 </HSVTweak>

			 <HSVTweak>
				 <SMax>50</SMax>
				 <HFact>0</HFact>
				 <HConst>50</HConst>
				 <SConst>120</SConst>
				 <VConst>-10</VConst>
			 </HSVTweak>

		 </Filters>
	 </Scheme>
	 <Scheme>
		 <Name>3am (Still Awake Mixxx)</Name>
		 <Filters>
			 <Invert/>
			 <Add>
				 <Amount>50</Amount>
			 </Add>
			 <ScaleWhite>
				 <Amount>1.5</Amount>
			 </ScaleWhite>
			 <Add>
				 <Amount>-50</Amount>
			 </Add>
		 </Filters>
	 </Scheme>
 </Schemes>

